<div class="header-sticky">
	<!-- Top Bar -->
	<div class="header-top">	
		<div class="clearfix hf-max-width">			
			<!-- Top Left -->
			<div class="top-left">
				<!-- Info List -->
				<ul class="info-list">
					<li><a href="mailto:info@skyvirt.tech"><fa-icon [icon]="faEnvelope"></fa-icon> <span class="mob-hide">info@skyvirt.tech</span></a></li>
					<li class="infolast"><a href="tel:+91-0294-2417300"><fa-icon [icon]="faPhoneSquare"></fa-icon> <span class="mob-hide">+91-0294-2417300</span></a></li>
				</ul>
			</div>
			
			<!-- Top Right -->
			<div class="top-right pull-right">
				<ul class="info-list"> <!--  social-box  -->
					<li *ngIf="loginName == null"><a routerLink="/login"><fa-icon [icon]="faSignInAlt"></fa-icon> <span class="mob-hide">Login</span></a></li>
					<li *ngIf="loginName == null"><a routerLink="/registration"><fa-icon [icon]="faUserCircle"></fa-icon> <span class="mob-hide">Registration</span></a></li>
					
					<!-- Wallet -->
					<li *ngIf="credit_system=='Active'" >
											<div ngbDropdown placement="bottom-right" class="d-inline-block user-note">
							<button class="btn btn-outline-primary user-note-btn" id="dropdownBasic1" ngbDropdownToggle><fa-icon [icon]="faWallet" class="discontents"></fa-icon> <sup class="ball-counter">{{wallet_balance}}</sup></button>
							<div ngbDropdownMenu aria-labelledby="dropdownBasic1" class="user_dropdown_box user_dropdown_menu text-center wmlh">
								<small>Total Credits : {{wallet_balance}}</small>
							</div>
						</div>
					</li>
					
					<li *ngIf="loginName != null" > 
						<div class="top-rside">
							<!-- Basket -->
							<div placement="bottom-left" ngbDropdown class="d-inline-block user-note">
								<button class="btn btn-outline-primary user-note-btn" id="dropdownBasic1" ngbDropdownToggle><fa-icon [icon]="faShoppingBasket" class="discontents"></fa-icon> <sup class="ball-counter">{{basket_count}}</sup></button>							
								<div ngbDropdownMenu aria-labelledby="dropdownBasic1" class="user_dropdown_box dropdown_box_width user_basketi">							
									<div class="basket-scroll">										
										<div *ngFor="let bs of basket; let i = index" class="basket">
											<a routerLink="/exercise-description/{{bs.alias}}" class="basket-heading">{{bs.name}}</a>
											<button (click)="removeBasket(bs.id);" class="closer" placement="bottom" ngbTooltip="Remove">
												<fa-icon [icon]="faTimesCircle"></fa-icon>
											</button>		
										</div>									

										<div *ngIf="basket.length==0" class="basket empty">
											<span class="basket-heading">Empty Basket</span>
										</div>
										
										<!-- -->
										<div *ngIf="basket.length!=0" class="basket">
											<div class="btn-box text-center mt-2">
												<div class="basket">
													<div class="btn-box text-right mt-2">
														<div class="theme-btn">
															<a routerLink="/basket" class="txt-white p-0"><span class="txt">View Basket</span></a> 
														</div>
													</div>															
												</div>
											</div>								
										</div>									
								</div>
								</div>
							</div>	
						</div>
					</li>
					
					<li *ngIf="loginName != null" > 
						<div class="top-rside">						  				
							<div ngbDropdown placement="bottom-right" class="d-inline-block user-note">
								<button class="btn btn-outline-primary user-note-btn" id="dropdownBasic1" ngbDropdownToggle>
								Hi, {{loginName}} <fa-icon [icon]="faBell" class="discontents"></fa-icon>  <sup class="ball-counter">{{note_count}}</sup></button>
								<div ngbDropdownMenu aria-labelledby="dropdownBasic1" class="user_dropdown_box dropdown_box_width">								
									<div (scroll)="onScroll($event)" class="notification-scroll">
										<div class="notification">
											<div *ngFor="let pro of process_array; let i = index" class="notify process-{{i}} bg-process pl-2 pr-2">
												<div class="notification-time pl-0 pr-0">
													<span class="float-left text-left label">{{pro.label}} - <span *ngIf="pro.status=='in_queue'; else pro_status" > processing</span><ng-template #pro_status > {{pro.status}}</ng-template></span>
													<span class="notification-date" placement="bottom" ngbTooltip="{{pro.date_time}}">{{pro.time_ago}}</span>
													<button *ngIf="pro.status!='in_queue' && pro.status!='processing'" (click)="remove_bgprocss_notify(i,pro.id)" class="corse pull-right" placement="bottom" ngbTooltip="Dismiss">
														<fa-icon [icon]="faTimesCircle"></fa-icon>
													</button>
												</div>

												<div class="progress">
													<div [ngClass]="{'progress-bar progress-bar-striped': true, 'progress-bar-animated': pro.status!='success', 'bg-success': pro.status=='success', 'bg-danger': pro.status=='fail'}" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%"></div>
												</div>
											</div>

											<div *ngFor="let note of notification; let i = index" class="notify">
												<div class="notification-time">
													<span class="notification-date" placement="bottom" ngbTooltip="{{note.date_time}}">{{note.time_ago}}</span>
													<button (click)="delete_notification(index,note.id)" class="corse" placement="bottom" ngbTooltip="Dismiss">
														<fa-icon [icon]="faTimesCircle"></fa-icon>
													</button>
												</div>									
												<a *ngIf="note.Slug!=null" routerLink="/{{note.Slug}}" class="notification-heading">
													{{note.Log_Narration}}								
												</a>	

												<span *ngIf="note.Slug==null"  >		
													{{note.Log_Narration}}	
												</span>
												
												<div *ngIf="note.Slug!=null" class="basket">
													<div class="btn-box text-center">
														<div class="basket">
															<div class="btn-box text-right">
																<div class="theme-btn">
																	<a routerLink="/{{note.Slug}}" class="txt-white p-0"><span class="txt">View</span></a> 
																</div>
															</div>																	
														</div>
													</div>								
												</div>									
											</div>
											
											<div *ngIf="notification.length==0" class="notify">
												<span class="notification-heading">
													Empty Notifications										
												</span>
											</div>																							
										</div>
									</div>
								</div>
							</div>
						</div>
					</li>
					<li *ngIf="loginName != null" >
						<div ngbDropdown placement="bottom-right" class="d-inline-block user-note">
							<button class="btn btn-outline-primary user-note-btn" id="dropdownBasic1" ngbDropdownToggle><fa-icon [icon]="faUserCircle" class="discontents"></fa-icon> <span class="mob-hide">My Account</span></button>
							<div ngbDropdownMenu aria-labelledby="dropdownBasic1" class="user_dropdown_box user_dropdown_menu">							
								
								<a *ngIf="loginGroupId=='2'" routerLink="/dd-instructor/dashboard" ngbDropdownItem>Dashboard</a>
								<a *ngIf="loginGroupId=='1'" routerLink="/dd-terminal/dashboard" ngbDropdownItem>Dashboard</a>

								<a *ngIf="loginGroupId=='3'" routerLink="/profile" ngbDropdownItem>Profile</a>
								<a *ngIf="loginGroupId=='3'" routerLink="/user-exercises" ngbDropdownItem>My Exercises</a>
								
								<a *ngIf="loginGroupId=='3'" routerLink="/my-activity" ngbDropdownItem>My Activity</a>
								<a *ngIf="loginGroupId=='3'" routerLink="/my-exercises-stats" ngbDropdownItem>My Exercises Stats</a>
								<a *ngIf="credit_system=='Active'" routerLink="/my-transactions/1" ngbDropdownItem>My Transactions</a>
								<a *ngIf="credit_system=='Active'" routerLink="/my-voucher/1" ngbDropdownItem>My Voucher</a>

								<a ngbDropdownItem  (click) = "logOut()" >Log off</a>
							</div>
						</div>
					</li>				
				</ul>
			</div>	
		</div>		
	</div>
	<!-- Top Bar End -->

